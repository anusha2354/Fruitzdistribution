<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fruitdistribution">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="FruitDistribution"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ItemList"></activity>
        <activity android:name=".AddDetail" />
        <activity android:name=".NewCustom" />
        <activity android:name=".CustomerLog" />
        <activity android:name=".AmountDetails" />
        <activity android:name=".ProductDetails" />
        <activity android:name=".Product" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>